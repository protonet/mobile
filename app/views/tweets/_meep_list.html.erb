<ul id="<%= "channel-#{channel.id}" %>" class="meeps">
  <%- tweets.each do |hash| -%>
    <%- hash.each do |user, meeps| -%>
      <%= render :partial => 'tweets/meep', :locals => {
        :meeps           => meeps,
        :avatar          => user.active_avatar_url,
        :author          => user.display_name,
        :channel_id      => channel.id
      } %>
    <%- end -%>
  <%- end -%>
</ul>